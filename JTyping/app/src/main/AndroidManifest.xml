<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.pro.pedro.jtyping">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_hearth"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo_hearth_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.JTyping">
        <activity
            android:name=".ListUsers"
            android:exported="true" />
        <activity
            android:name=".EmptyActivity"
            android:exported="true"
            android:label="@string/title_activity_empty"
            android:theme="@style/Theme.JTyping.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.JTyping.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>